{
    "manifest_version" :2,
    
	"name": "Proof Assistant",
	
	"description": "A proof assistant",
	
	"version": "0.1",

	"icons": {
		"16": "images/proof16.png",
		"48": "images/proof48.png",
		"128": "images/proof128.png"
	},
	
	"app": {
		"launch": {
			"local_path":"ProofAssistant.html"
		}
	},
    
	"minimum_chrome_version": "12.0.706.0",
	
	"permissions":[
		"unlimitedStorage",
		"tabs",
		"notifications"
	]
}